<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="webmember">
	 
	 <select id="userIdCheck" resultType="WebMemberVO" parameterType="map">
	 	select * 
	 	from webmember
		where mem_id=#{userId} 
		and mem_pass=#{password}
	 </select>
	 
</mapper>






